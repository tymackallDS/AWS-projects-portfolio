#!usr/bin/env python

import boto3
print("deleting objects in bucket")

#initialize a session using s3
s3 = boto3.resource('s3')

def delete_objects(bucket_name):
    try:
        # Select the bucket
        bucket = s3.Bucket(bucket_name)
        
        # Iterate over all objects in the bucket
        for obj in bucket.objects.all():
            # Delete the object
            obj.delete()
            print(f"Deleted {obj.key}")
        
        print(f"All objects in bucket '{bucket_name}' have been deleted.")
    except Exception as e:
        print(f"Error: {e}")

# Replace 'your-bucket-name' with your actual bucket name
delete_objects('your-bucket-name: ')